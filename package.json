{
  "name": "fastify-cache-control",
  "version": "1.0.0",
  "description": "Declarative HTTP Cache-Control headers management for Fastify with CDN support, presets, and pattern matching",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "node --import tsx --test test/plugin.test.ts",
    "test:coverage": "node --import tsx --test --experimental-test-coverage test/plugin.test.ts",
    "lint": "eslint src test",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "fastify",
    "fastify-plugin",
    "cache",
    "cache-control",
    "http-cache",
    "caching",
    "headers",
    "cdn",
    "cdn-cache-control",
    "rfc9213"
  ],
  "author": "fracabu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fracabu/fastify-cache-control.git"
  },
  "bugs": {
    "url": "https://github.com/fracabu/fastify-cache-control/issues"
  },
  "homepage": "https://github.com/fracabu/fastify-cache-control#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "fastify-plugin": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "fastify": "^5.0.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "fastify": "^5.0.0"
  }
}
